{
  "name": "cta-components",
  "private": true,
  "version": "0.0.1",
  "main": "dist/cta-components.umd.js",
  "module": "dist/cta-components.es.js",
  "license": "UNLICENSED",
  "exports": {
    ".": {
      "import": "./dist/cta-components.es.js",
      "require": "./dist/cta-components.umd.js"
    }
  },
  "types": "types/index.d.ts",
